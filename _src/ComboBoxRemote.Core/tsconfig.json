{
    "compileOnSave": true,
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": false,
        "removeComments": false,
        "declaration": true,
        "declarationDir": "node_modules/@types/",
        "sourceMap": true,
        "target": "es5",
        "lib": [ "ES5", "DOM" ],
        "outDir": "wwwroot",
        "rootDir": "./tscripts"
    },
    "include": [
        "tscripts/**/*"
    ],
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}
